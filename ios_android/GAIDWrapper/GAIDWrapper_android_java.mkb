#!/usr/bin/env mkb
# Builder mkb file for the java portion of the GAIDWrapper extension on android
platform JAVA

files
{
    (source/android)
    GAIDWrapper.java
}

librarypath "lib/android/gps/google-play-services.jar"
librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/GAIDWrapper.jar
